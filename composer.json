{
    "name": "stumason/claudavel",
    "description": "Opinionated Laravel starter setup: Horizon, Reverb, Telescope, Sanctum, health checks, coding standards, and a concurrent dev script.",
    "keywords": ["laravel", "horizon", "reverb", "telescope", "starter", "opinionated"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stu Mason",
            "email": "stu@stuartmason.co.uk"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.3",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Stumason\\Claudavel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stumason\\Claudavel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "lint": "pint",
        "analyse": "phpstan analyse"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stumason\\Claudavel\\ClaudavelServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
